--Gui Loadout
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Dizzy Hub By Dizz", "DarkTheme")
 
 
--Versatile Scripts Section
local Versatile = Window:NewTab("Versatile Scripts")
local Section = Versatile:NewSection("Click to Execute")
 
 
--Versatile Scripts Buttons
Section:NewButton("Infinite Yield", "A FE Admin Commands Script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
 
Section:NewButton("CMD X", "A FE Admin Commands Script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end)
 
Section:NewButton("Universal Esp", "Esp that can be used in many different games", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/fernandoagoncalves/universal-esp/main/esp.lua", true))()
end)
 
Section:NewButton("Universal Aimbot", "Aimbot that can be used in many different games", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Blissful4992/Miscellaneous/main/UNIVERSAL.lua"))()
end)
 
if game.PlaceId == 4872321990 then
 
    --Islands Script Section
    local Islands = Window:NewTab("Islands")
    local Section = Islands:NewSection("Click to Execute")
 
    --Islands Scripts Buttons
    Section:NewButton("System Exodus Islands", "A Islands Script", function()
        loadstring(game:HttpGet('https://system-exodus.com/scripts/Islands/Islands.lua', true))()
    end)
end
